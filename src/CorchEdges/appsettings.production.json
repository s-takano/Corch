{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=prod-sql-server;Database=CorchEdgesProd;User Id=prod-user;Password=prod-password-placeholder;TrustServerCertificate=true",
    "CacheConnection": "prod-redis-server:6379"
  },
  "ExternalServices": {
    "ApiBaseUrl": "https://api.example.com",
    "ApiKey": "prod-api-key-placeholder",
    "Timeout": "00:01:00"
  },
  "Features": {
    "EnableDetailedErrors": false,
    "EnableSwagger": false,
    "EnableSeedData": false
  },
  "Email": {
    "SmtpServer": "smtp.example.com",
    "SmtpPort": 587,
    "Username": "noreply@example.com",
    "Password": "prod-email-password-placeholder",
    "EnableSsl": true
  },
  "Security": {
    "JwtSecret": "prod-jwt-secret-key-placeholder",
    "TokenExpirationMinutes": 30
  },
  "HealthChecks": {
    "Enabled": true,
    "DetailedOutput": false
  },
  "Monitoring": {
    "ApplicationInsightsKey": "app-insights-key-placeholder",
    "EnablePerformanceCounters": true
  }
}