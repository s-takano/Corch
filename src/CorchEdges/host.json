{
  "$schema": "https://json.schemastore.org/host",
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "Function": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Information",
      "Microsoft.Azure.Functions.Worker": "Information",
      "Microsoft.Identity.Client": "Warning",
      "Microsoft.Identity.Client.*": "Warning",
      "MSAL": "Warning",
      "default": "Information"
    }
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    },
    "serviceBus": {
      "prefetchCount": 0,
      "maxConcurrentCalls": 1,
      "autoCompleteMessages": true
    },
    "timers": {
      "timezone": "Tokyo Standard Time",
      "scheduleMonitor": {
        "useMonitor": true
      }
    }
  }
}